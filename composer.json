{
	"name": "rndwiga/credit_bureau",
	"type": "package",
	"description": "A package to manage credit bureau integration",
	"keywords": ["api", "rndwiga","laravel"],
	"authors": [
		{
			"name": "Raphael Ndwiga",
			"email": "raphndwi@gmail.com"
		}
	],
	"require": {
